{
  "expo": {
    "name": "paragomierz",
    "slug": "paragomierz",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "paragomierz"
    },
    "android": {
      "package": "com.facebook.react.devsupport.DevSettingsActivity"
    },
    "entryPoint": "node_modules/expo/AppEntry.js",
    "description": "Aplikacja umożliwia zrobienie zdęcia paragonu lub wrzucenie zdjęcia z pamięci urządzenia, edycję listy produktów i synchronizację wydatków z kontomierzem."
  }
}
